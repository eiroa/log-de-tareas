<header class="jumbotron subhead" id="overview">
    <div>
      <div class="well centerlogin">
	<form method='get' action='/auth/developer/callback' noValidate='noValidate'>
		    <%= csrf_token_field %>

	<label for='name'>Name:</label>
	<input class="span4" type='text' id='name' name='name' placeholder="somebody"/>
	<label for='email'>Email:</label>
	<input class="span4" type='text' id='email' name='email' placeholder="somebody@someplace.com"/><br/>
	<button type='submit' name="submit" id="submit">Sign In</button>      

	</form>

    </div>
  </div>
</header>



