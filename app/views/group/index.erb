<span class="span6">

  
	<% if @list.empty? %>
		 <p>No tienes grupos creados</p>
		<% else %>

			<h1>Estos son tus grupos</h1>
			<ul>
			  <% @list.each do |group| %>
			  <li><%=group.name %></li>
			  <% end %>
      </ul>
		<% end %>

  <p>     
  <input type="button" onClick="document.location = 'group_create'"
  value="Crear nuevo grupo" class="btn btn-inverse btn-large form_exp_submit">
  </span>
	</p>

  
  <p> <%=@message%> </p>
